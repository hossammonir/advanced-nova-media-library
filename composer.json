{
    "name": "digitaltunnel/nova-media-library",
    "description": "Media Library for Laravel Nova using Spatie Media Library",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "symfony/polyfill-php83": "^1.28",
        "spatie/laravel-medialibrary": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.8",
        "larastan/larastan": "^2.4",
        "laravel/nova": "^5.0",
        "laravel/nova-devtool": "^1.0.1",
        "laravel/pint": "^1.17",
        "orchestra/pest-plugin-testbench": "^2.0",
        "orchestra/testbench": "^8.24|^9.2",
        "phpstan/phpstan": "^1.11",
        "spatie/laravel-ray": "^1.39"
    },
    "autoload": {
        "psr-4": {
            "DigitalTunnel\\NovaMediaLibrary\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DigitalTunnel\\NovaMediaLibrary\\NovaMediaLibraryServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
